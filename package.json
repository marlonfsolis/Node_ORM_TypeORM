{
  "name": "node-express-ts-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dist": "node dist/bin/www.js",
    "start:dev": "set DEBUG=node-express-ts-api:* & nodemon src/bin/www.ts",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build:test": "tsc --build",
    "migrate:create": "npx typeorm-ts-node-esm migration:create",
    "migrate:generate": "npx typeorm-ts-node-esm migration:generate -d src/typeorm/dataSource.ts",
    "migrate:run": "npx typeorm-ts-node-esm migration:run -d src/typeorm/dataSource.ts",
    "migrate:revert": "npx typeorm-ts-node-esm migration:revert -d src/typeorm/dataSource.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.4",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
